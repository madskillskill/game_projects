    "import inspirobot\n",
    "import requests\n",
    "from PIL import Image\n",
    "from time import sleep\n",
    "\n",
    "for i in range(10):\n",
    "    url = inspirobot.generate()\n",
    "    data = requests.get(url).content\n",
    "    f = open((str(i)+'.jpg'),'wb')\n",
    "    f.write(data)\n",
    "    f.close()\n",
    "    sleep(10)"
   ]
  }
